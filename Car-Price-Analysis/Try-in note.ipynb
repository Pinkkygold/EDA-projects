   "source": [
    "# Fuction for unit conversion\n",
    "# kmpl = km/kg by ~1.33\n",
    "\n",
    "\n",
    "def unit_conversion(mileage, unit):\n",
    "    if unit == 'km/kg':\n",
    "        return unit * 1.33\n",
    "    else:\n",
    "        return mileage\n",
    "\n",
    "\n",
    "df['Mileage(kmpl)'] = df.apply(lambda x: unit_conversion(x['Mileage(kmpl)'], x['Unit']), axis=1) "
   ]
